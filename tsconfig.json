{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "outDir": "out",
        "lib": [
            "esnext"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ]
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
